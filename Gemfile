source 'https://rubygems.org'

gem 'rails', '3.2.11'
gem 'thin'
gem 'inherited_resources'
gem 'bootstrap-sass'
gem 'quiet_assets'

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails',   '~> 3.2.3'
  gem 'coffee-rails', '~> 3.2.1'
  gem 'uglifier', '>= 1.0.3'
end

group :development do
  gem 'sqlite3'
end

group :production do
  gem 'pg'
end

gem 'jquery-rails'


